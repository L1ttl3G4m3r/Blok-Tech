<!DOCTYPE html>
<html lang="nl">
  <head>
    <%- include('partials/head') %>
  </head>
  <body>
    <header><%- include('partials/header') %></header>
    <main>
      <section class="profile-section">
        <%- include('partials/profile-photo') %>
        <%- include('partials/upload-photo') %>

        <div id="profileData" data-role="visitor">
          <% if (!isArtist) { %>
            <%- include('partials/profile-data') %>
          <% } %>
        </div>
        <div id="profileDataArtist" data-role="artist">
          <% if (isArtist) { %>
            <%- include('partials/profile-data-artist') %>
          <% } %>
        </div>

      </section>
    </main>
    <footer>
      <%- include('partials/nav') %>
    </footer>
    <script>window.userRole = "<%= user.role %>";</script>
  <script src="/static/script/script.js"></script>
  </body>
</html>
