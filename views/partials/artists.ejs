<ul class="artist-list">
  <% if (typeof artists !== 'undefined' && Array.isArray(artists) && artists.length > 0) { %>
    <% artists.forEach(artist => { %>
      <li class="artist-item">
        <div class="artist-image">
          <a href="/artist/<%= artist._id %>">
          <img src="<%= artist.artistImages || 'static/icons/profile/avatar-stock.svg' %>" alt="<%= artist.username %>">
          </a>
        </div>
        <h4 class="artist-name"><%= artist.username %></h4>
      </li>
    <% }); %>
  <% } else { %>
    <li>Geen artiesten gevonden</li>
  <% } %>
</ul>
